        "build": "tsc",
        "build:watch": "tsc -w"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kelsny/class-splitter.git"
    },
    "type": "module",
    "keywords": [
        "typescript",
        "tooling",
        "synth"
    ],
    "author": "kelsny <cursors.owo.uwu@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kelsny/class-splitter/issues"
    },
    "homepage": "https://github.com/kelsny/class-splitter#readme",
    "devDependencies": {
        "@types/chai": "^4.3.1",
        "@types/mocha": "^9.1.1",
        "@types/node": "^17.0.35",
        "chai": "^4.3.6",
        "chokidar-cli": "^3.0.0",
        "mocha": "^10.0.0",
        "ts-node": "^10.8.0",
        "typescript": "^4.7.2"
    },
    "dependencies": {}
