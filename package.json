{
    "name": "@kelsny/class-splitter",
    "version": "0.0.1",
    "description": "Decouple your monolithic classes into separate maintainable files.",
    "bin": {},
    "main": "dist/index.js",
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "fmt": "yarn format",
        "format": "prettier --write .",
        "test": "mocha",
